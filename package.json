{
  "name": "@keenable/mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Keenable AI search, fetch, and feedback APIs",
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "keenable-mcp-server": "./build/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "start:http": "node build/http-server.js",
    "dev": "tsc && node build/index.js",
    "dev:http": "tsc && node build/http-server.js",
    "example:http": "tsc && node build/examples/http-client.js",
    "test:http": "tsc && node build/examples/test-http-server.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "keenable",
    "search",
    "web-search",
    "ai",
    "llm"
  ],
  "author": "Keenable AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keenableai/keenable-mcp-ts"
  },
  "homepage": "https://github.com/keenableai/keenable-mcp-ts#readme",
  "bugs": {
    "url": "https://github.com/keenableai/keenable-mcp-ts/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0"
  }
}
